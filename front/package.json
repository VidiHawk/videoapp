    "start": "npm run compile & npm run dev-server",
    "compile": "webpack --progress --colors --watch",
    "dev-server": "cross-env NODE_APP=development cross-env NODE_PATH=./src concurrently --kill-others \"nodemon src/server\" ",
    "demo": "npm run demo-compile && npm run demo-run",
    "demo-compile": "NODE_APP='demo' webpack --config webpack.config.prod.babel.js --progress --colors",
    "demo-run": "cross-env NODE_APP=demo cross-env NODE_PATH=./src concurrently --kill-others \"nodemon src/server\" ",
    "prod": "npm run prod-compile && npm run prod-run",
    "prod-compile": "NODE_APP='production' webpack --config webpack.config.prod.babel.js --progress --colors",
    "prod-run": "cross-env NODE_APP=production cross-env NODE_PATH=./src concurrently --kill-others \"nodemon src/server\" "
